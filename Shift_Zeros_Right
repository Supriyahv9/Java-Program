public class Shift_Zeros_Right {
	
	public static void main(String[] args) {
		
		int[] a={1,2,3,0,3,0,4,3,2,1,0};
		int[] array= new int[a.length];
		int first=0;
		for(int i=0;i<a.length-1;i++) {
			if(a[i]!=0) {
				array[first++]=a[i];
			}
		}
		
		System.out.println(Arrays.toString(array));
	}

}
