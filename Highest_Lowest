public class Highest_Lowest {
	
public static void main(String[] args) {
		
		int[] a= {1,23,2,5,6,3,7};
		int max=a[0];
		int min=a[0];
		
		for(int i=1;i<a.length;i++) {
			if(a[i]>max) {
				max=a[i];
			}
			
			if(a[i]<min) {
				min=a[i];
			}
		}
		
		System.out.println(max);
		System.out.println(min);
		
		
	}

}
