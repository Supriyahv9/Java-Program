public class Sum_ofElements_Array {
	
	public static void main(String[] args) {
		
		int[] a= {1,2,3};
		int[] b= {3,2,1};
		int[] result=new int[a.length];
		System.out.println(a.length);
		
		for(int i=0;i<result.length;i++) {
			if(i<a.length) {
				result[i]+=a[i];
			}
			
			if(i<b.length) {
				result[i]+=b[i];
			}
		}
		
		System.out.println(Arrays.toString(result));
	}

}
